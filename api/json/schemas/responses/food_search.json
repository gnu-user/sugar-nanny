{
    "$schema": "http://json-schema.org/draft-04/schema#",

    "type": "object",

    "properties":
    {
        "success":   { "type": "boolean" },
        "code":      { "type": "integer" },
        "status":    { "type": "string" },
        "data":
        {
            "type": "array",
            "items":
            [
                {
                    "type": "object",
                    "properties":
                    {
                        "food_id":        { "type": "integer" },
                        "product_name":   { "type": "string" }
                    },
                    "required": ["food_id", "product_name"]
                }
            ]
        },
        "errors":    { "type": "array" }
    },
    "required": ["success", "code", "status"]
}
